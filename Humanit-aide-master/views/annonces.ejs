  <body>

      
    <div class="card" style="width: 20rem;left: 500px;">

      <div class="card-body " style="font-family: 'Libre Baskerville', serif;">
        
        <p class="card-text" >"Nous ne pouvons pas aider tout le monde, mais tout le monde peut aider quelq'un"</p> 
        </style>
        
        <h4 class="card-title">Ronald Regan</h4>
        
      </div>
      </div>
    </div>

    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12" style="font-family: 'Libre Baskerville', serif;">
          <h1 style = "padding-top: 20px;padding-bottom: 20px;text-align: center;width:1300px; ">
            Annonces disponible
          </h3>
        </div>
      </div>

    <%
    if(userData.length!=0){
    var i=1;
    userData.forEach(function(data){
    %>
    <div class="card-header"style="width: 350px;margin-left: 490px;margin-top: 20px;">
      <h4 class="my-0 font-weight-normal">Publi√© par <%=data.username %> </h4>
    </div>
    <div class="card-body"style="width: 350px;margin-left: 490px;margin-top: 20px;">
      <h2 class="card-title pricing-card-title"><%=data.type %></h2>
      <ul class="list-unstyled mt-3 mb-4">
        <h3>
        <small class="text-muted">Telephone : <%=data.telephone %></small><br>
        <small class="text-muted">Region : <%=data.region%></small><br>
        <small class="text-muted" style="font-family: 'Asap', sans-serif; font-size: large; ">Description : <%=data.description %> </small><br>
      </h3>
      </ul>
     
    </div>
    <%  i++; }) %>
    <% } else{ %>
      <div class="card-header">
        <h4 class="my-0 font-weight-normal">Pas d'annonce !</h4>
      </div>
      <% } %>
    
    
  </body>
